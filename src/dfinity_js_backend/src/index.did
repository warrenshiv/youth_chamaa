    addMember: (record {name:text; email:text}) -> (variant {Ok:record {id:text; contributions:vec text; name:text; investments:vec text; email:text}; Err:variant {InvalidPayload:text; NotFound:text}});
    getContributions: () -> (vec record {id:text; memberId:text; date:text; amount:nat64}) query;
    getMembers: () -> (vec record {id:text; contributions:vec text; name:text; investments:vec text; email:text}) query;
}
